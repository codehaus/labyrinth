<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>	
  <class name="org.codehaus.labyrinth.om.ProjectBlock" table="LProjectBlock">
    <id name="id" type="int" column="rfLProjectBlock">
      <generator class="native"/>
    </id>
    <property not-null="true" name="projectId" column="rfLProject"/>
    <property not-null="true" name="blockId" column="rfLBlock"/>
  </class>
</hibernate-mapping>