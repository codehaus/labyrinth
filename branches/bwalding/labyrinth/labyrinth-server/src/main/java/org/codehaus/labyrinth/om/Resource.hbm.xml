<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>	
  <class name="org.codehaus.labyrinth.om.Resource" table="LResource">
    <id name="id" type="int" column="rfLResource">
      <generator class="native"/>
    </id>
    <property not-null="true" name="url" column="txUrl"/>
  </class>
</hibernate-mapping>