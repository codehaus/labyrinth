<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>	
  <class name="org.codehaus.labyrinth.om.ProjectBlockProperty" table="LProjectBlockProperty">
    <id name="id" type="int" column="rfLProjectBlockProperty">
      <generator class="native"/>
    </id>
    <property not-null="true" name="projectBlockId" column="rfLProjectBlock"/>
    <property not-null="true" name="name" column="txName"/>
    <property not-null="true" name="value" column="txValue"/>
  </class>
</hibernate-mapping>